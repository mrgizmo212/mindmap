generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Flow {
  id          String   @id @default(cuid())
  name        String   @unique
  description String?
  nodes       Json     @default("[]")
  edges       Json     @default("[]")
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}